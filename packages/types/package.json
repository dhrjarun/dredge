{
  "name": "dredge-types",
  "version": "0.5.0",
  "description": "Types for dredge, Internal package",
  "author": "Dhiraj Arun (https://github.com/dhrjarun)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dhrjarun/dredge.git",
    "directory": "packages/types"
  },
  "keywords": ["dredge", "types"],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf node_modules .turbo dist",
    "test": "vitest run --typecheck",
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "pnpm biome format ./source",
    "lint:fix": "pnpm biome format --write ./source"
  },
  "devDependencies": {
    "@types/node": "^20.12.4"
  },
  "dependencies": {
    "arktype": "2.0.0-rc.15",
    "superstruct": "^2.0.2",
    "valibot": "^0.42.1",
    "yup": "^1.4.0",
    "zod": "^3.22.4"
  }
}
